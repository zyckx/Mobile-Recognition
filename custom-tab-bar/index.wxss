
.tabbar-wrap{
	position: relative;
	z-index: 0;
}
.bg{
	width: 50px;
	height: 38px;
	border-radius: 20px;
	background-color: var(--td-tab-bar-active-bg, var(--td-brand-color-light, var(--td-primary-color-1, #f2f3ff)));
	position: absolute;
	z-index: -1;
	/* 86px */
	left: 18px;
	transition:all ease-in-out 0.7s;
}