<!--pages/result/result.wxml-->
<view class="container">
	<view class="sensorName">
		<t-input label="传感器名称" placeholder="传感器名称" bindchange="inputChange" data-id="sensorName" disabled value="{{sensorName}}"></t-input>
	</view>
	<view class="sensorDate">
		<t-input label="时间" value="{{startTime+'--'+endTime}}" disabled />
	</view>
	<view class="computeType">
		<t-input label="计算类型" value="{{computeType}}" disabled />
	</view>
	<view class="result">
		<t-input label="结果" value="{{data}}" disabled />
	</view>
	<view class="buttonGroup">
		<t-button theme="primary" block size="medium" class="btn reset" bindtap="onBack">返回</t-button>
	</view>
</view>