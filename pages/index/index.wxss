/**index.wxss**/
.container {
	width: 100%;
	margin-top: 20%;
	padding: 5%;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: flex-start;
	justify-content: space-around;
	/* justify-self: start; */
}

.sensorName {
	margin-bottom: 30rpx;
}

.sensorDate {
	width: 100%;
	display: flex;
	flex-wrap: nowrap;
	align-items: center;
	justify-content:space-around;
	margin-bottom: 30rpx;
}
.sensorDate .dateSelect{
	width: 50rpx;
	white-space: nowrap;
	transform: translateX(19rpx);
}
.sensorDate .datePicker{
	width: 260rpx;
}

.computeType {
	width: 100%;
}

.buttonGroup {
	display: flex;
	flex-wrap: nowrap;
}

.buttonGroup .btn {
	width: 300rpx;
	margin: 20rpx;
}